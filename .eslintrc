{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "arrow-body-style": [
      "warn",
      "always"
    ],
    "operator-linebreak": "off",
    "react/prop-types": [
      2
    ],
    "react/destructuring-assignment": [
      "warn"
    ],
    "react/no-deprecated": "off",
    "react/jsx-wrap-multilines": "off"
  },
  "extends": [
    "airbnb"
  ]
}